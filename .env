# API URLs
REACT_APP_API_URL=https://api.amoo.app
REACT_APP_SOCKET_URL=wss://socket.amoo.app

# Feature flags
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_ANALYTICS=false

# Cache settings
REACT_APP_CACHE_DURATION=86400 # 24 hours in seconds
REACT_APP_MAX_CACHE_ITEMS=100

# Image settings
REACT_APP_MAX_IMAGE_SIZE=5242880 # 5MB in bytes
REACT_APP_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif 